<h1>Listing earnings</h1>

<table>
  <thead>
    <tr>
      <th>Deal</th>
      <th>Amount</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% earnings.each do |earning| %>
      <tr>
        <td><%= earning.id %></td>
        <td><%= number_to_currency(earning.amount) %></td>
        <td><%= earning.created_at %></td>
        <td><%= link_to 'Show', earning %></td>
      </tr>
    <% end %>
  </tbody>
</table>
