<h1>Listing landing_pages</h1>

<table>
  <thead>
    <tr>
      <th>Deal</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @landing_pages.each do |landing_page| %>
      <tr>
        <td><%= landing_page.deal_id %></td>
        <td><%= landing_page.url %></td>
        <td><%= link_to 'Show', landing_page %></td>
        <td><%= link_to 'Edit', edit_landing_page_path(landing_page) %></td>
        <td><%= link_to 'Destroy', landing_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Landing page', new_landing_page_path %>
